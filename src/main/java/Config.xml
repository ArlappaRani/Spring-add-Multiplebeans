<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<!-- Setter injection -->
<bean class="com.spring.create3objects.SpringProject2.Laptop" id="lap1">

<property name="serialNo" value="1"></property>
<property name="brand" value="Dell"></property>
<property name="price" value="55555.5F"></property>
</bean>

<bean class="com.spring.create3objects.SpringProject2.Laptop" id="lap2">

<property name="serialNo" value="2"></property>
<property name="brand" value="Hp"></property>
<property name="price" value="66666.6F"></property>
</bean>
<!-- Constructor injection-->

<bean class="com.spring.create3objects.SpringProject2.Laptop" id="lap3">

<constructor-arg value="3"></constructor-arg>
<constructor-arg value="Lenovo"></constructor-arg>
<constructor-arg value="75000.5F"></constructor-arg>
</bean>

</beans>
